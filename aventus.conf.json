{
	"build": [
        {
            "name": "main",
            "version": "0.0.1",
            "namespace": "TodoList",
            "componentPrefix": "av",
            "inputPath": [
                "./src/*"
            ],
            "outputFile": "./dist/main.js",
            "generateDefinition": true,
            "includeBase": true
        }
    ],
    "static": [
        {
            "name": "static",
            "inputPath": "./src/static/",
            "outputPath": "./dist/"
        }
    ]
}